
### Using generated keystore
sudo keytool -genkeypair -keysize 2048 -alias activeeonkeystore -keypass proactive -keystore activeeonKeyStore -storepass proactive
sudo keytool -exportcert -keystore activeeonKeyStore -alias activeeonkeystore -keypass proactive -file activeeon.der
sudo keytool -import -keystore $JAVA_HOME/jre/lib/security/cacerts -file activeeon.crt -alias activeeonkeystore

### Using PA keystore
keytool -export -alias jetty -keystore $path_to_keystore -file keystore.crt
keytool -import -keystore $PA_HOME/jre/lib/security/cacerts -file $path_to_keystore.crt

### TGT
curl -k  -X POST -d "username=scheduler&password=scheduler_pwd" https://localhost:8443/cas/v1/tickets
